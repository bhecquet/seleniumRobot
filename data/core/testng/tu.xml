<!DOCTYPE suite SYSTEM "http://beust.com/testng/testng-1.0.dtd" >
<suite name="Unit test suite" parallel="methods" verbose="1" thread-count="1">
   
	<parameter name="implicitWaitTimeOut" value="2"></parameter>
    

    <test name="Unit tests" parallel="false">
    
    	<parameter name="variables" value="{'key6':'value6', 'key2': 'value20'}" />
        
        <groups>
            <run>
                <include name="ut"/>
            </run>
        </groups>

        <packages>
            <package name="com.seleniumtests.ut.*" />
        </packages>
    </test>
    
    <test name="Integration tests" parallel="false">
    
        
        <groups>
            <run>
                <include name="it"/>
            </run>
        </groups>

        <packages>
            <package name="com.seleniumtests.it.*" />
        </packages>
    </test>

</suite> 
